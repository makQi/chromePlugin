{  
  "manifest_version": 2,
  "name": "MyTaskList",		// 插件名字
  "version": "0.1.0", // 版本号
  "description": "Management my everyday's task lists.",  // 描述
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
   },
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "My Task List",
    "default_popup": "popup.html"
  }  
}